[app]
gpio = 27
impulse = 0.01

[mqtt]
id = raspi
name = raspi with several sensors
homieversion = 4.0
broker = localhost
port = 1883
# exposed nodes, separated by ","" for multiple ones
nodes = reed
username = mosqitto
password = *****
insecure = True
qos = 1
retain_message = True
# Retry to connect to mqtt broker
mqttretry = 5
# how often should be a publish to MQTT (in Seconds)
publishtime = 120
